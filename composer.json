{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "abraham/twitteroauth": "^4.0",
        "ajcastro/eager-load-pivot-relations": "^0.2.2",
        "algolia/algoliasearch-client-php": "^3.4",
        "algolia/scout-extended": "^3.1",
        "barryvdh/laravel-dompdf": "^2.2",
        "diglactic/laravel-breadcrumbs": "*",
        "doctrine/dbal": "^4.0.2",
        "embed/embed": "^4.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^1.3",
        "intervention/image": "^2.7",
        "itsgoingd/clockwork": "^5.1",
        "joelbutcher/laravel-facebook-graph": "^1.2",
        "laragear/recaptcha": "^2.1",
        "laravel/framework": "^11.0",
        "laravel/scout": "^10.10",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "~3.0",
        "lorisleiva/laravel-actions": "^2.4",
        "nyholm/psr7": "^1.4",
        "owen-it/laravel-auditing": "^13.0",
        "php-http/message-factory": "^1.1",
        "romanzipp/laravel-queue-monitor": "^5.2",
        "rootinc/laravel-azure-middleware": "^0.9.10",
        "socialiteproviders/linkedin": "^4.2",
        "spatie/laravel-analytics": "^5.3",
        "spatie/laravel-html": "^3.2",
        "spatie/laravel-json-api-paginate": "^1.10",
        "spatie/laravel-menu": "^4.0",
        "spatie/laravel-query-builder": "^5.0",
        "stevebauman/purify": "^6.2",
        "symfony/filesystem": "^6.0",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "spatie/laravel-ignition": "^2.8",
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/lang": "*",
        "laravel-lang/publisher": "*",
        "laravel/sail": "^1.4",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/BackendClass.php",
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }   
}
